{
  "name": "koreografeye",
  "description": "A minimal orchestrator and policy executor component for the Mellon Researcher Pod project",
  "version": "0.2.1",
  "homepage": "",
  "license": "MIT",
  "author": "Patrick Hochstenbach <Patrick.Hochstenbach@UGent.be>",
  "lsd:module": true,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "orch": "bin/orch --keep rules/demo/*.n3",
    "orch:blogic": "bin/orch --keep --config config-blogic.jsonld rules/blogic/00_demo.n3",
    "orch:policy": "bin/orch --keep --config config-blogic.jsonld rules/blogic/00_policy.n3 rules/blogic/policy/*.n3",
    "orch:solid": "bin/orch --keep --config config-solid.jsonld rules/solid/*.n3",
    "pol": "bin/pol --keep",
    "watch": "tsc --watch",
    "build": "npm run build:ts && npm run build:components",
    "build:ts": "tsc ; chmod 755 dist/*js",
    "build:components": "componentsjs-generator -s dist",
    "clean": "rm err/* ; rm out/err/*",
    "clean:all": "npm run clean ; rm in/* out/* out/ok/*"
  },
  "bin": {
    "orch": "dist/orch.js",
    "pol": "dist/pol.js"
  },
  "files": [
    "components",
    "dist/**/*.d.ts",
    "dist/**/*.js",
    "dist/**/*.js.map"
  ],
  "dependencies": {
    "@comunica/query-sparql-rdfjs": "^2.5.1",
    "commander": "^9.4.1",
    "componentsjs": "^5.3.2",
    "eyereasoner": "^2.0.0",
    "is-hidden-file": "^1.1.2",
    "jsonld": "^8.1.0",
    "log4js": "^6.7.0",
    "n3": "^1.16.3",
    "node-fetch": "^3.3.0",
    "rdf-parse": "^2.1.1",
    "rdf-serialize": "^2.0.1",
    "stream-to-string": "^1.2.0",
    "streamify-string": "^1.0.1",
    "tmp": "^0.2.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/jsonld": "^1.5.8",
    "@types/n3": "^1.10.4",
    "@types/nodemailer": "^6.4.6",
    "@types/streamify-string": "^1.0.0",
    "@types/tmp": "^0.2.3",
    "componentsjs-generator": "^3.1.0",
    "nodemailer": "^6.8.0",
    "tsconfig.json": "^1.0.11",
    "typescript": "^4.9.3"
  }
}
